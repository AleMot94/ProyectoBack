{{#if productos.length}}

<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">producto</th>
      <th scope="col">precio</th>
      <th scope="col">thumbnail</th>
    </tr>
  </thead>
  <tbody>
    {{#each productos}}
    <tr>
      <td>{{this.producto}}</td>
      <td>{{this.precio}}</td>
      <td>{{this.thumbnail}}</td>
    </tr>
    {{/each}}
    
  </tbody>
</table>
{{ else }}
<h3 class="alert alert-warning">No hay personas</h3>
{{/if}}
