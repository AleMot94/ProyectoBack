<h1>productos</h1>


<div class="table-responsive">
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>producto</th>
            <th>precio</th>
            <th>thumbnail</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>
                {{this.producto}}
            </td>
            <td>
                {{this.precio}}
            </td>
            <td>
                {{this.thumbnail}}
            </td>
        </tr>
        {{/each}}
    </table>
</div>